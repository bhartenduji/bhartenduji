{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Uvicorn (Reload)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "justMyCode": true,
      "args": [
        "app.main:app",
        "--reload",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "console": "integratedTerminal"
    }
  ]
}